{
  "expo": {
    "name": "HelloWorld",
    "slug": "HelloWorld",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "helloworld",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.helloworld.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "eas": {
      "projectId": "4f59419f-1284-4e34-8027-7d7042b94f0a"
    },
    "facebookAppId": "1601328111314351",
    "facebookClientToken": "4f52dead5d618f1273bddfab82db77bb",
    "facebookDisplayName": "HelloWorld",
    "facebookScheme": "fb1601328111314351",
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "expo-router",
      "expo-apple-authentication",
      [
        "react-native-fbsdk-next",
        {
          "appID": "1601328111314351",
          "clientToken": "4f52dead5d618f1273bddfab82db77bb",
          "displayName": "HelloWorld",
          "scheme": "fb1601328111314351",
          "advertiserIDCollectionEnabled": false,
          "autoLogAppEventsEnabled": false,
          "isAutoInitEnabled": true,
          "iosUserTrackingPermission": "This identifier will be used to deliver personalized ads to you."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you set a profile picture for the groom or bride."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableSeparateBuildPerCPUArchitecture": true
          }
        }
      ],
      "react-native-fbsdk-next"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4f59419f-1284-4e34-8027-7d7042b94f0a"
      }
    }
  }
}